{
  "expo": {
    "name": "plants-app",
    "slug": "plants-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "app-plantas",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hugopessoa.appplantas"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "96d6f69a-407b-449f-a185-8bdc5564b04c"
      },
      "router": {
        "origin": false
      }
    },
    "ios": {
      "bundleIdentifier": "com.hugopessoa.appplantas"
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "petsi"
  }
}